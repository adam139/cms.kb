<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="cms.db"
    purge="False">
    
    <configlet
        title="System settings"
        action_id="cms.db.sys_settings"
        appId="cms.db" 
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@types-settings"
        visible="True"
        i18n:attributes="title">
            <permission>cms.policy:Set system</permission>
    </configlet>

</object>